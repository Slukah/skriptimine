#!/bin/bash
#Kontrollib, kas antud arv on algarv

function on_alg_arv() {
    local arv=$1

    #Kas arv on v채iksem kui 2
    if [ "$arv" -lt 2 ]; then
        echo "$arv - ei ole algarv"
        return
    fi

    #M채채rab kas on algarv
    for (( jagaja=2; jagaja*jagaja<=arv; jagaja++ )); do
        if (( arv % jagaja == 0 )); then
            echo "$arv - ei ole algarv"
            return
        fi
    done

    echo "$arv - on algarv"
}

#Kontrollime 20 esimest t채isarvu
for (( arv=1; arv<=20; arv++ )); do
    on_alg_arv "$arv"
done
